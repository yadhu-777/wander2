<% layout("./layout/boilerplate.ejs") %>
<body>

  <h4 class="mb-4">ALL LISTINGS</h4>
    <div class="row row-cols-lg-4 row-cols-md-3 row-cols-sm-1">
        <% for(dt of data) { %>
          <a href="/listings/<%=dt._id%>" class="listy">
            <div class="card col " >
             
                <img src="<%=dt.image.url%>" class="card-img-top" alt="...">
              <div class="card-body">
              <p><%=dt.title%> </p>
                
              </div>
          
            </div>
          </a>
            <%}%>
    </div>
</body>
